---
import navLinks from "@data/navbarLinks.json";
---

<nav class="flex items-center gap-x-6">
  <a href="/" class="">
    <img src="/logo-dark.png" alt="midudev logo" class="w-36 mb-1 bg-midu-600 rounded-3xl dark:bg-transparent" />
  </a>
  <ul class="flex items-center justify-center gap-4">
    {
      navLinks.map((link) => (
        <li class="">
          <a href={link.url} class="dark:text-midu-50 font-bold">
            {link.title}
          </a>
        </li>
      ))
    }
  </ul>
</nav>
